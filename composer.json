{
    "name": "darshan/exceptionemail",
    "description": "An easy way to send emails with stack trace whenever an exception occurs on the server for Laravel Applications.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel8", "exceptions", "email", "laravel"],
    "authors": [
        {
            "name": "Darshan Baraiya",
            "email": "damku999@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "8.*",
        "illuminate/view": "8.*",
        "illuminate/config": "8.*",
        "illuminate/mail": "8.*",
        "illuminate/log": "8.*",
        "illuminate/queue": "8.*",
        "illuminate/console": "8.*",
        "symfony/error-handler": "~5.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Darshan\\ExceptionEmail\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Darshan\\ExceptionEmail\\ExceptionEmailServiceProvider"
            ]
        }
    }
}
